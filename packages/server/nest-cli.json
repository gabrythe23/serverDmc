{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "compilerOptions": {
    "webpack": false
  },
  "projects": {
    "authentication": {
      "type": "library",
      "root": "libs/authentication",
      "entryFile": "index",
      "sourceRoot": "libs/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "libs/authentication/tsconfig.lib.json"
      }
    },
    "fast-decorator": {
      "type": "library",
      "root": "libs/fast-decorator",
      "entryFile": "index",
      "sourceRoot": "libs/fast-decorator/src",
      "compilerOptions": {
        "tsConfigPath": "libs/fast-decorator/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "exception": {
      "type": "library",
      "root": "libs/exception",
      "entryFile": "index",
      "sourceRoot": "libs/exception/src",
      "compilerOptions": {
        "tsConfigPath": "libs/exception/tsconfig.lib.json"
      }
    },
    "health-check": {
      "type": "library",
      "root": "libs/health-check",
      "entryFile": "index",
      "sourceRoot": "libs/health-check/src",
      "compilerOptions": {
        "tsConfigPath": "libs/health-check/tsconfig.lib.json"
      }
    },
    "starter": {
      "type": "library",
      "root": "libs/starter",
      "entryFile": "index",
      "sourceRoot": "libs/starter/src",
      "compilerOptions": {
        "tsConfigPath": "libs/starter/tsconfig.lib.json"
      }
    },
    "util": {
      "type": "library",
      "root": "libs/util",
      "entryFile": "index",
      "sourceRoot": "libs/util/src",
      "compilerOptions": {
        "tsConfigPath": "libs/util/tsconfig.lib.json"
      }
    },
    "server-dmc": {
      "type": "application",
      "root": "apps/server-dmc",
      "entryFile": "main",
      "sourceRoot": "apps/server-dmc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/server-dmc/tsconfig.app.json"
      }
    },
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "describe": {
      "type": "library",
      "root": "libs/describe",
      "entryFile": "index",
      "sourceRoot": "libs/describe/src",
      "compilerOptions": {
        "tsConfigPath": "libs/describe/tsconfig.lib.json"
      }
    }
  },
  "monorepo": true
}